<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="akka" type="Akka.Configuration.Hocon.AkkaConfigurationSection, Akka"/>
	</configSections>

	<akka>
		<hocon>
			<![CDATA[
          akka {
  actor {
     actor.provider = cluster
  }
  remote {
    dot-netty.tcp {
      hostname = "localhost"
      port = 5000
    }
  }
  cluster {
    seed-nodes = ["akka.tcp://ClusterSystem@localhost:5000"]
    roles = ["frontend"]
  }
}
			      ]]>
		</hocon>
	</akka>
</configuration>
